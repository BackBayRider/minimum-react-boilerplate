{
  "env": {
    "mocha": true
  },
  "plugins": [
    "react",
    "babel"
  ],
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-no-bind": [2, {
      "ignoreRefs": true,
      "allowArrowFunctions": false,
      "allowBind": false
    }],
    "no-param-reassign": [2, {"props": false}],
    "react/jsx-indent-props": 0,
    "max-len": [2, 150, 4],
    "new-cap": 0,
    "semi": 0
  }
}
